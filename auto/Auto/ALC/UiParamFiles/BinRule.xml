<?xml version="1.0" encoding="utf-8"?>
<Root xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Rules>
    <Rule Order="1">
      <Name>F_Bin</Name>
      <ConditionGroup Type="And">
        <Conditions>
          <Condition Reverse="false">
            <Name>ContainsAll</Name>
            <Param>
              <int>2</int>
              <int>3</int>
            </Param>
          </Condition>
        </Conditions>
        <ConditionGroups />
      </ConditionGroup>
      <Result>4</Result>
    </Rule>
    <Rule Order="2">
      <Name>P_Bin</Name>
      <ConditionGroup Type="And">
        <Conditions>
          <Condition Reverse="false">
            <Name>None</Name>
          </Condition>
        </Conditions>
        <ConditionGroups />
      </ConditionGroup>
      <Result>Max</Result>
    </Rule>
    <Rule Order="0">
      <Name>Fail_All</Name>
      <ConditionGroup Type="And">
        <Conditions>
          <Condition Reverse="false">
            <Name>ContainsOne</Name>
            <Param>
              <int>4</int>
            </Param>
          </Condition>
        </Conditions>
        <ConditionGroups />
      </ConditionGroup>
      <Result>4</Result>
    </Rule>
  </Rules>
</Root>